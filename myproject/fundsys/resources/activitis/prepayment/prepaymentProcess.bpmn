    <sequenceFlow id="flow13" name="提交财务" sourceRef="usertask6" targetRef="usertask9"></sequenceFlow>
    <sequenceFlow id="flow14" name="归档" sourceRef="usertask9" targetRef="usertask8"></sequenceFlow>
    <sequenceFlow id="flow15" name="归档" sourceRef="usertask4" targetRef="usertask8"></sequenceFlow>
    <sequenceFlow id="flow16" name="审批通过，流程结束" sourceRef="usertask8" targetRef="endevent1"></sequenceFlow>
